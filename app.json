{
  "expo": {
    "name": "proxie",
    "slug": "proxie",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/bottombar/stream.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/bottombar/stream.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.proxie"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "owner": "raldweb",
    "plugins": [
      [
        "expo-barcode-scanner",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access camera."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "3f189f36-3d00-45b4-93d6-1f42b7530a71"
      }
    }
  }
}
